@model IEnumerable<GradProj.Models.ResultModel>

@{
    ViewBag.Title = "Results";
}

<h2>Results</h2>

<table  class="table table-striped table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ResultId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InstitutionId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssignmentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Score)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberofAttendance)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.ResultId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.InstitutionId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.AssignmentName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Score)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.NumberofAttendance)
    </td>
</tr>
}

</table>
